{
   "background": {
      "scripts": [ "lib/jquery/jquery.min.js", "lib/bitcoinjs-min.js", "lib/chrome_storage.js", "lib/sparecoins.js", "lib/sparecoins_lib.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "icons/icon19.png",
      "default_popup": "index.html",
      "default_title": "Omnicoin Wallet"
